{
  "name": "dynamic-mock-api",
  "version": "1.2.0",
  "description": "A Node module for creating a dynamic mock (express) API on the fly. Allows users to monitor requests to the API. Works well with browser driven UI testing.",
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "scripts": {
    "test": "mocha --exit",
    "build": "tsc && tsc --project ts_spec_config.json"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "build/lib/*",
    "package-lock.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barton603/dynamic-mock-api.git"
  },
  "keywords": [
    "test",
    "browser",
    "api",
    "mock",
    "http",
    "protractor",
    "ui",
    "verify",
    "webdriver",
    "selenium"
  ],
  "author": "Chris Barton <bartonchrisj@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/barton603/dynamic-mock-api/issues"
  },
  "homepage": "https://github.com/barton603/dynamic-mock-api#readme",
  "dependencies": {
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "http-errors": "^1.6.3",
    "morgan": "^1.9.0",
    "require-glob": "^3.2.0",
    "ts-deferred": "^1.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/express": "^4.11.1",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "tslint": "^5.9.1"
  }
}
